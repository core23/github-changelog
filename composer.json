{
    "name": "localheinz/github-changelog",
    "description": "A script that generates a change log based on specified references.",
    "authors": [
        {
            "name": "Andreas MÃ¶ller",
            "email": "am@localheinz.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "beberlei/assert": "^2.6.4",
        "knplabs/github-api": "^1.4.14",
        "symfony/console": "^3.1.5",
        "symfony/stopwatch": "^3.1.5"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "0.3.2",
        "phpunit/phpunit": "^5.6.1",
        "refinery29/php-cs-fixer-config": "0.4.20",
        "refinery29/test-util": "0.9.0"
    },
    "autoload": {
        "psr-4": {
            "Localheinz\\GitHub\\ChangeLog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Localheinz\\GitHub\\ChangeLog\\Test\\": "test"
        }
    },
    "bin": [
        "github-changelog"
    ]
}
